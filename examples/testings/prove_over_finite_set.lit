let s set:
    s := {1, 2, 3}

prove_by_enum:
    forall x s:
        x > 0

let s2 set:
    s2 := {}

# any factual statement is true on empty set
prove_by_enum:
    forall x s2:
        x > 0
        x < 0

let s3, s4 set:
    s3 := {1, 2, 3}
    s4 := {1, 2, 3}

prove_by_enum:
    forall x s3, y s4:
        x * y >= 1