fn_template fn_with_three_args():
    fn (i N_pos) obj:
        dom:
            i <= 3

fn three_dim_tuple(x, y, z set) fn_with_three_args():
    x <= 3
    =>:
        x(1) $in a
        x(2) $in b
        x(3) $in c

let x fn_with_three_args():
    x = three_dim_tuple(R, C, Q)

x(1) $in R # x at dimension 1 is in R
x(2) $in C # x at dimension 2 is in C
x(3) $in Q # x at dimension 3 is in Q
