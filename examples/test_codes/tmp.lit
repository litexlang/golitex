prop is_max_element_of(S set, m R):
    $is_subset_of(S, R)
    m $in S
    forall x S:
        x <= m

prop is_min_element_of(S set, m R):
    $is_subset_of(S, R)
    m $in S
    forall x S:
        m <= x

