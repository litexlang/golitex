"
claim:
    forall original_rice R:
        0.7 * original_rice + 90 = original_rice
        =>:
            original_rice = 300
    prove:
        know 0.7 * original_rice + 90 = original_rice
        know 90 = original_rice - 0.7 * original_rice
        90 = 0.3 * original_rice
        0.3 * original_rice = 90
        original_rice = 90 / 0.3 = 300
"

let a R
# a = 0.7 * a + 0.3 * a 
a - 0.7 * a = 0.3 * a 